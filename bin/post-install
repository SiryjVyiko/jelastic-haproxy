#!/bin/bash -eu

source /etc/jelastic/environment

[ -f /opt/repo/log/.gitignore ] && { rm -f /opt/repo/log/.gitignore; }
[ -f /opt/repo/versions/$Cartridge_Version/conf/haproxy.conf ] && { cp -f /opt/repo/versions/$Cartridge_Version/conf/haproxy.conf /opt/repo/versions/$Cartridge_Version/conf/haproxy.conf.OLD; }

cp -f /opt/repo/versions/$Cartridge_Version/haproxy.conf.default /opt/repo/versions/$Cartridge_Version/conf/haproxy.conf
