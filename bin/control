#!/bin/sh

start(){
/etc/init.d/haproxy start;
}

stop(){
/etc/init.d/haproxy stop;
}

restart(){
/etc/init.d/haproxy restart;
}

reload(){
/etc/init.d/haproxy reload;
}

condrestart(){
/etc/init.d/haproxy condrestart;
}

status(){
/etc/init.d/haproxy status;
}

check(){
/etc/init.d/haproxy check;
}
